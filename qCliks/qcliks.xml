<?xml version="1.0" encoding="UTF-8"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
  author="Włodzimierz Bartczak"
  icon=""
  version="0.1_2020-12-20"
  shortdescription="Quick cliks"
  pl.shortdescription="Szybkie tagowanie"
  description="Quick cliks for many features"
  pl.description="Szybkie tagowanie dla wielu cech">
  <!-- usuwa fixme-->
 <group name="qCliks" icon="">
    <!--Tworzy staw-->
    <item name="Pond" pl.name="Staw" icon="pond.svg" type="closedway">
    <key key="natural" value="water" />
    <key key="water" value="pond" />
    </item>
    <!--Tworzy przepust-->
    <item name="Culvert" pl.name="Przepust" icon="presets/transport/passage/tunnel_culvert.svg" type="way">
    <key key="tunnel" value="culvert" />
    <key key="layer" value="-1" />
    </item>
    <!--Tworzy most i warstwę-->
    <item name="Bridge" pl.name="Most" icon="bridge.png" type="way">
    <key key="bridge" value="yes" />
    <key key="layer" value="1" />
    </item>
    <!--Tworzy podjazd do posesji-->
    <item name="Driveway" pl.name="Podjazd" icon="" type="way">
    <key key="highway" value="service" />
    <key key="service" value="driveway" />
    </item>
    <!--Tworzy obszar szuwar-->
    <item name="Reedbed" pl.name="Szuwary" icon="" type="closeway">
    <key key="natural" value="wetland" />
    <key key="wetland" value="reedbed" />
    </item>
    <!--Usuwa przestarzałe znaczniki-->
    <item name="is_in" pl.name="is_in" icon="is_in.png" type="node">
    <key key="is_in:county" />
    <key key="is_in:municipality" />
    <key key="is_in:province" />
    <key key="is_in:village" />
    <key key="is_in" />
    <key key="is_in:city" />
    </item>
    <!--Usuwa tagi adresowe z budynków-->
    <item name="no_adress" icon="no_adres.png" type="closedway">
    <key key="addr:city" />
    <key key="addr:city:simc" />
    <key key="addr:housenumber" />
    <key key="addr:postcode" />
    <key key="addr:street" />
    <key key="addr:street:sym_u" />
    </item>
</group>

</presets>